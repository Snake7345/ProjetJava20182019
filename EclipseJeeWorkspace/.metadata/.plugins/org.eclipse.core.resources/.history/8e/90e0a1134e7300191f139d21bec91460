package venteVoiture.connection;

import java.io.Serializable;
import javax.persistence.*;

/**
 * The primary key class for the adresse database table.
 * 
 */
@Embeddable
public class AdressePK implements Serializable {
	//default serial version id, required for serializable classes.
	private static final long serialVersionUID = 1L;

	@Column(unique=true, nullable=false)
	private int idAdresse;

	@Column(insertable=false, updatable=false, unique=true, nullable=false)
	private int CP_IdCP;

	public AdressePK() {
	}
	public int getIdAdresse() {
		return this.idAdresse;
	}
	public void setIdAdresse(int idAdresse) {
		this.idAdresse = idAdresse;
	}
	public int getCP_IdCP() {
		return this.CP_IdCP;
	}
	public void setCP_IdCP(int CP_IdCP) {
		this.CP_IdCP = CP_IdCP;
	}

	public boolean equals(Object other) {
		if (this == other) {
			return true;
		}
		if (!(other instanceof AdressePK)) {
			return false;
		}
		AdressePK castOther = (AdressePK)other;
		return 
			(this.idAdresse == castOther.idAdresse)
			&& (this.CP_IdCP == castOther.CP_IdCP);
	}

	public int hashCode() {
		final int prime = 31;
		int hash = 17;
		hash = hash * prime + this.idAdresse;
		hash = hash * prime + this.CP_IdCP;
		
		return hash;
	}
}