package venteVoiture.connection;

import java.io.Serializable;
import javax.persistence.*;


/**
 * The persistent class for the voiture database table.
 * 
 */
@Entity
@Table(name="voiture")
@NamedQuery(name="Voiture.findAll", query="SELECT v FROM Voiture v")
public class Voiture implements Serializable {
	private static final long serialVersionUID = 1L;

	@EmbeddedId
	private VoiturePK id;

	@Column(nullable=false)
	private float prix;

	//bi-directional many-to-one association to Couleur
	@ManyToOne
	@JoinColumn(name="Couleur_IdCouleur", nullable=false, insertable=false, updatable=false)
	private Couleur couleur;

	//bi-directional many-to-one association to Entrepot
	@ManyToOne
	@JoinColumn(name="Entrepot_idEntrepot", nullable=false, insertable=false, updatable=false)
	private Entrepot entrepot;

	//bi-directional many-to-one association to Modele
	@ManyToOne
	@JoinColumns({
		@JoinColumn(name="Modele_IdModele", referencedColumnName="IdModele", nullable=false, insertable=false, updatable=false),
		@JoinColumn(name="Modele_Marque_IdMarque", referencedColumnName="Marque_IdMarque", nullable=false, insertable=false, updatable=false)
		})
	private Modele modele;

	//bi-directional many-to-one association to Package
	@ManyToOne
	@JoinColumn(name="Package_IdPackage", nullable=false, insertable=false, updatable=false)
	private Package package;

	public Voiture() {
	}

	public VoiturePK getId() {
		return this.id;
	}

	public void setId(VoiturePK id) {
		this.id = id;
	}

	public float getPrix() {
		return this.prix;
	}

	public void setPrix(float prix) {
		this.prix = prix;
	}

	public Couleur getCouleur() {
		return this.couleur;
	}

	public void setCouleur(Couleur couleur) {
		this.couleur = couleur;
	}

	public Entrepot getEntrepot() {
		return this.entrepot;
	}

	public void setEntrepot(Entrepot entrepot) {
		this.entrepot = entrepot;
	}

	public Modele getModele() {
		return this.modele;
	}

	public void setModele(Modele modele) {
		this.modele = modele;
	}

	public Package getPackage() {
		return this.package;
	}

	public void setPackage(Package package) {
		this.package = package;
	}

}