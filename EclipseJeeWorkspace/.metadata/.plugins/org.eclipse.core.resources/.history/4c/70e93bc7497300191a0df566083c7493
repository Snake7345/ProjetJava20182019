package venteVoiture.connection;

import java.io.Serializable;
import javax.persistence.*;


/**
 * The persistent class for the voiture_options_voiture database table.
 * 
 */
@Entity
@Table(name="voiture_options_voiture")
@NamedQuery(name="VoitureOptionsVoiture.findAll", query="SELECT v FROM VoitureOptionsVoiture v")
public class VoitureOptionsVoiture implements Serializable {
	private static final long serialVersionUID = 1L;

	@EmbeddedId
	private VoitureOptionsVoiturePK id;

	public VoitureOptionsVoiture() {
	}

	public VoitureOptionsVoiturePK getId() {
		return this.id;
	}

	public void setId(VoitureOptionsVoiturePK id) {
		this.id = id;
	}

}